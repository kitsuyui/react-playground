{
  "name": "react-playground",
  "version": "0.0.15",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "turbo run dev --parallel",
    "dev": "turbo run dev --parallel",
    "build": "turbo build",
    "test": "turbo test",
    "test:vrt": "turbo test:vrt",
    "test:vrt:update": "turbo test:vrt:update",
    "test:vrt:install": "turbo test:vrt:install",
    "format:prettier": "prettier --write .",
    "format:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore"
  },
  "workspaces": [
    "config/*",
    "packages/*",
    "examples/*"
  ],
  "dependencies": {
    "@kitsuyui/eslint-config": "link:./config/eslint-config-custom"
  },
  "devDependencies": {
    "eslint-plugin-unused-imports": "^3.0.0",
    "turbo": "^1.10.13"
  }
}
