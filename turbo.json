{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": [
        "^dev"
      ]
    },
    "@kitsuyui/react-clock#build": {
      "outputs": [
        "packages/react-clock/dist/**"
      ]
    },
    "@kitsuyui/react-timer#build": {
      "outputs": [
        "packages/react-timer/dist/**"
      ]
    },
    "@kitsuyui/react-stopwatch#build": {
      "outputs": [
        "packages/react-stopwatch/dist/**"
      ]
    },
    "@kitsuyui/react-dekamoji#build": {
      "outputs": [
        "packages/react-dekamoji/dist/**"
      ]
    },
    "@kitsuyui/react-measure#build": {
      "outputs": [
        "packages/react-measure/dist/**"
      ]
    },
    "@kitsuyui/react-treemap#build": {
      "outputs": [
        "packages/react-treemap/dist/**"
      ]
    },
    "@kitsuyui/react-textfield#build": {
      "outputs": [
        "packages/react-textfield/dist/**"
      ]
    },
    "@kitsuyui/react-wavebox#build": {
      "outputs": [
        "packages/react-wavebox/dist/**"
      ]
    },
    "@kitsuyui/react-components#build": {
      "dependsOn": [
        "@kitsuyui/react-clock#build",
        "@kitsuyui/react-timer#build",
        "@kitsuyui/react-stopwatch#build",
        "@kitsuyui/react-dekamoji#build",
        "@kitsuyui/react-textfield#build",
        "@kitsuyui/react-measure#build",
        "@kitsuyui/react-treemap#build",
        "@kitsuyui/react-wavebox#build"
      ],
      "outputs": [
        "packages/react-components/dist/**"
      ]
    },
    "@kitsuyui/react-style-bulma#build": {
      "dependsOn": [
        "@kitsuyui/react-components#build"
      ],
      "outputs": [
        "packages/react-style-bulma/dist/**"
      ]
    },
    "storybook#build": {
      "dependsOn": [
        "@kitsuyui/react-components#build",
        "@kitsuyui/react-style-bulma#build"
      ],
      "outputs": [
        "packages/storybook/dist/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "packages/*/dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "test:vrt": {
      "dependsOn": [
        "^test:vrt:install",
        "^test:vrt:update"
      ]
    },
    "test:vrt:update": {
      "outputs": [
        "examples/storybook/tests/**/*.png"
      ]
    },
    "test:vrt:install": {},
    "format:eslint": {},
    "format:prettier": {
      "dependsOn": [
        "format:eslint"
      ]
    },
    "lint:type": {},
    "lint:eslint": {},
    "lint:prettier": {},
    "format": {
      "dependsOn": [
        "format:eslint",
        "format:prettier"
      ]
    },
    "lint": {
      "dependsOn": [
        "lint:type",
        "lint:eslint",
        "lint:prettier"
      ]
    }
  }
}