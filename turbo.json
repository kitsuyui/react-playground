{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["^dev"]
    },
    "@kitsuyui/react-clock#build": {
      "outputs": ["packages/clock/dist/**"]
    },
    "@kitsuyui/react-tab#build": {
      "outputs": [
        "packages/tab/dist/**"
      ]
    },
    "@kitsuyui/react-timer#build": {
      "outputs": ["packages/timer/dist/**"]
    },
    "@kitsuyui/react-stopwatch#build": {
      "outputs": ["packages/stopwatch/dist/**"]
    },
    "@kitsuyui/react-dekamoji#build": {
      "outputs": ["packages/dekamoji/dist/**"]
    },
    "@kitsuyui/react-measure#build": {
      "outputs": ["packages/measure/dist/**"]
    },
    "@kitsuyui/react-treemap#build": {
      "outputs": ["packages/treemap/dist/**"]
    },
    "@kitsuyui/react-editablelabel#build": {
      "dependsOn": ["@kitsuyui/react-textfield#build"],
      "outputs": ["packages/editablelabel/dist/**"]
    },
    "@kitsuyui/react-textfield#build": {
      "outputs": ["packages/textfield/dist/**"]
    },
    "@kitsuyui/react-wavebox#build": {
      "outputs": ["packages/wavebox/dist/**"]
    },
    "@kitsuyui/react-binary#build": {
      "outputs": ["packages/binary/dist/**"]
    },
    "@kitsuyui/react-components#build": {
      "dependsOn": [
        "@kitsuyui/react-binary#build",
        "@kitsuyui/react-clock#build",
        "@kitsuyui/react-tab#build",
        "@kitsuyui/react-timer#build",
        "@kitsuyui/react-stopwatch#build",
        "@kitsuyui/react-dekamoji#build",
        "@kitsuyui/react-textfield#build",
        "@kitsuyui/react-measure#build",
        "@kitsuyui/react-treemap#build",
        "@kitsuyui/react-wavebox#build"
      ],
      "outputs": ["packages/components/dist/**"]
    },
    "@kitsuyui/react-style-bulma#build": {
      "dependsOn": ["@kitsuyui/react-components#build"],
      "outputs": ["packages/style-bulma/dist/**"]
    },
    "storybook#build": {
      "dependsOn": [
        "@kitsuyui/react-components#build",
        "@kitsuyui/react-binary#build",
        "@kitsuyui/react-clock#build",
        "@kitsuyui/react-tab#build",
        "@kitsuyui/react-timer#build",
        "@kitsuyui/react-stopwatch#build",
        "@kitsuyui/react-dekamoji#build",
        "@kitsuyui/react-textfield#build",
        "@kitsuyui/react-measure#build",
        "@kitsuyui/react-treemap#build",
        "@kitsuyui/react-wavebox#build",
        "@kitsuyui/react-editablelabel#build",
        "@kitsuyui/react-style-bulma#build"
      ],
      "outputs": ["packages/storybook/dist/**"]
    },
    "build": {
      "dependsOn": ["^build", "typedoc"]
    },
    "test:vrt": {
      "dependsOn": ["^test:vrt:install", "^test:vrt:update"]
    },
    "test:vrt:update": {
      "outputs": ["examples/storybook/tests/**/*.png"]
    },
    "test:vrt:install": {},
    "typedoc": {}
  }
}
