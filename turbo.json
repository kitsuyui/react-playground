{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "start": {
      "dependsOn": ["^start"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["examples/simple/build/**", "packages/*/dist/**"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:vrt": {}
  }
}
